{
  "schemaVersion": 1,
  "id": "iceandfire",
  "version": "${version}",
  "name": "Ice And Fire Community Edition",
  "description": "An unofficial fork of Ice And Fire.\nDragons and other mythological creatures in minecraft.",
  "authors": [
    "Alexthe666 (Origin Author)",
    "IAFEnvoy"
  ],
  "contact": {
    "sources": "https://github.com/IAFEnvoy/IceAndFire-CE"
  },
  "license": "LGPL-3.0",
  "icon": "logo.jpg",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.iafenvoy.iceandfire.fabric.IceAndFireFabric"
    ],
    "client": [
      "com.iafenvoy.iceandfire.fabric.client.IceAndFireFabricClient"
    ],
    "emi": [
      "com.iafenvoy.iceandfire.compat.emi.IceAndFireEmiPlugin"
    ],
    "jade": [
      "com.iafenvoy.iceandfire.compat.jade.IceAndFireJadePlugin"
    ],
    "cardinal-components-entity": [
      "com.iafenvoy.iceandfire.fabric.component.ModComponentEntry"
    ],
    "modmenu": [
      "com.iafenvoy.iceandfire.fabric.ModMenu"
    ]
  },
  "custom": {
    "cardinal-components": [
      "iceandfire:entity_data",
      "iceandfire:portal_data"
    ]
  },
  "mixins": [
    "iceandfire.mixins.json"
  ],
  "accessWidener": "iceandfire.accesswidener",
  "depends": {
    "minecraft": "1.20.1",
    "jupiter": ">=2",
    "uranus": ">=2"
  }
}
